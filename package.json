{
  "name": "mafsv",
  "version": "0.1.1",
  "description": "Vue Component for interactive math, reproduction of mafs",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.prod.js",
  "jsdelivr": "dist/index.prod.js",
  "types": "es/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib",
    "volar.d.ts"
  ],
  "keywords": [
    "mafs",
    "component library",
    "vue"
  ],
  "scripts": {
    "dev": "pnpm -C playground dev",
    "build": "rollup --config rollup.config.js && tsc -b --force tsconfig.esm.json && tsc -b --force tsconfig.cjs.json",
    "gen-volar-dts": "esbuild scripts/gen-volar.js --bundle --platform=node --tsconfig=./tsconfig.esbuild.json | node"
  },
  "author": "faga",
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "deepmerge": "^4.2.2",
    "esbuild": "^0.16.17",
    "eslint": "^8.31.0",
    "rollup": "^3.9.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-sass": "^1.12.17",
    "sass": "^1.57.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.45",
    "@vueuse/core": "^9.10.0",
    "vue": "^3.2.45"
  }
}
